{
  "name": "Noty",
  "version": "1.0.0",
  "description": "Simple autosaving sticky note",
  "main": "dist/main.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean": "npm-run-all -p clean:dist",
    "build:main": "webpack --config webpack/main.ts",
    "build:renderer": "webpack --config webpack/renderer.ts",
    "build:server": "webpack --config webpack/server.ts",
    "build:all": "npm-run-all -p build:main build:renderer build:server",
    "start:main": "electron dist/main.js",
    "start:server": "node dist/server.js & webpack --config webpack/renderer.ts --watch",
    "start": "HOT=1 npm-run-all -p start:main start:server",
    "dev": "npm-run-all build:all start"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "homepage": "https://github.com/fabiospampinato/noty",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/noty"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/noty/issues"
  },
  "keywords": [
    "electron",
    "react",
    "webpack",
    "codemirror",
    "sticky",
    "note"
  ],
  "devDependencies": {},
  "dependencies": {
    "awesome-typescript-loader": "^3.2.2",
    "chalk": "^2.0.1",
    "conf-merge": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.6.11",
    "electron-about-window": "^1.6.1",
    "electron-debug": "^1.3.0",
    "electron-is": "^2.4.0",
    "electron-store": "^1.2.0",
    "electron-window-state": "^4.1.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.0",
    "isomorphic-style-loader": "^3.0.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "node-libs-browser": "^2.0.0",
    "node-localstorage": "^1.3.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "pify": "^3.0.0",
    "react": "^15.6.1",
    "react-codemirror": "^1.0.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.1.0",
    "typescript": "^2.4.2",
    "typings": "^2.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.2.1",
